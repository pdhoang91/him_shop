<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="csrf-token" content={csrf_token_value()}>

	<!-- theme meta -->
	<meta name="theme-name" content="revolve" />

	<!--Favicon-->
	<link rel="shortcut icon" href="images/favicon.ico" type="image/x-icon">

  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/app.css")}/>

  	<!-- THEME CSS
	================================================== -->
	<!-- Bootstrap -->
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/bootstrap/css/bootstrap.min.css")}/>
	<!-- Themify -->
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/themify/css/themify-icons.css")}/>
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/slick-carousel/slick-theme.css")}/>
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/slick-carousel/slick.css")}/>
	<!-- Slick Carousel -->
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/owl-carousel/owl.carousel.min.css")}/>
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/owl-carousel/owl.theme.default.min.css")}/>
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/plugins/magnific-popup/magnific-popup.css")}/>
	<!-- manin stylesheet -->
  <link phx-track-static rel="stylesheet" href={Routes.static_path(@conn, "/assets/css/style.css")}/>


  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/app.js")}></script>

    	<!-- THEME JAVASCRIPT FILES
================================================== -->
	<!-- initialize jQuery Library -->
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/jquery/jquery.js")}></script>
	<!-- Bootstrap jQuery -->
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/bootstrap/js/bootstrap.min.js")}></script>
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/bootstrap/js/popper.min.js")}></script>
	<!-- Owl caeousel -->
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/owl-carousel/owl.carousel.min.js")}></script>
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/slick-carousel/slick.min.js")}></script>
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/magnific-popup/magnific-popup.js")}></script>
	<!-- Instagram Feed Js -->
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/instafeed-js/instafeed.min.js")}></script>
	<!-- Google Map -->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCC72vZw-6tGqFyRhhg5CkF2fqfILn2Tsw"></script>
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/plugins/google-map/gmap.js")}></script>
	<!-- main js -->
  <script defer phx-track-static type="text/javascript" src={Routes.static_path(@conn, "/assets/js/custom.js")}></script>



  </head>
  <body>
    <%= @inner_content %>
  </body>
</html>
